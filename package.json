{
  "name": "umi-pro",
  "displayName": "Umi Pro",
  "description": "Write Umi like a Pro! ",
  "version": "0.1.3",
  "license": "MIT",
  "engines": {
    "vscode": "^1.33.0"
  },
  "publisher": "DiamondYuan",
  "repository": "https://github.com/umijs/vscode-extension-umi-pro",
  "activationEvents": [
    "workspaceContains:package.json"
  ],
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "commit": "git-cz",
    "package": "vsce package",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "eslint --ext .js,.ts .",
    "travis-publish": "vsce publish -p $VS_TOKEN",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "pre-commit": [
    "lint"
  ],
  "egg": {
    "typescript": true
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Umi Pro configuration",
      "properties": {
        "umi_pro.quotes": {
          "type": "string",
          "default": "single",
          "description": "type of quotes",
          "enum": [
            "double",
            "single",
            "backtick"
          ]
        }
      }
    }
  },
  "devDependencies": {
    "@diamondyuan/eslint-config-typescript": "^6.0.0",
    "@types/babel-generator": "^6.25.3",
    "@types/babel-types": "^7.0.7",
    "@types/mocha": "^2.2.42",
    "@types/mz": "^0.0.32",
    "@types/node": "^10.12.21",
    "@types/power-assert": "^1.5.0",
    "@types/sinon": "^7.0.11",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.1.1",
    "conventional-changelog-cli": "^2.0.21",
    "decache": "^4.5.1",
    "egg-bin": "^4.13.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "glob": "^7.1.3",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.0",
    "remap-istanbul": "^0.13.0",
    "sinon": "^7.3.2",
    "typescript": "^3.4.5",
    "vsce": "^1.59.0",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "@babel/generator": "^7.4.4",
    "@babel/parser": "^7.4.4",
    "@babel/types": "^7.4.4",
    "globby": "^9.2.0",
    "mz": "^2.7.0"
  }
}
